{
  "name": "symbiote3",
  "version": "2.0.0",
  "description": "Track your master races.",
  "author": {
    "name": "Alex McCombie",
    "url": "https://lspacelabs.com"
  },
  "private": false,
  "license": "MIT",
  "keywords": [
    "express",
    "javascript",
    "react",
    "sqlite",
    "typescript"
  ],
  "browserslist": [
    "last 7 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0",
    "yarn": ">=1.1.0"
  },
  "main": "src/index.tsx",
  "prox": "http://localhost:4001",
  "scripts": {
    "build": "node ./buildScript",
    "build-front": "react-scripts build",
    "eject": "react-scripts eject",
    "start-server": "nodemon server/server.js --watch server/*",
    "start-front": "react-scripts start",
    "start": "concurrently \"npm run start-server\" \"npm run start-front\" --kill-others --kill-others-on-fail",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "axios": "0.21.2",
    "body-parser": "1.19.0",
    "bootstrap": "^5.1.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "helmet": "3.22.0",
    "knex": "0.20.13",
    "react": "^18.0.0",
    "react-bootstrap": "^2.2.3",
    "react-collapse": "^5.1.1",
    "react-dom": "16.13.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "concurrently": "5.1.0",
    "nodemon": "2.0.3",
    "react-scripts": "3.4.1",
    "sqlite3": "4.1.1",
    "typescript": "3.8.3"
  }
}
